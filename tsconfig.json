{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "moduleResolution": "node",
    "declaration": true,
    "lib": ["WebWorker"],
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "include":[
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
